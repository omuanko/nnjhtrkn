;
;<<<< shinzou >>>>
;
shnini:
	lda	#$ff
	sta	shnflg
shnmrt:	rts
;
;
shnmov:	;shinzou move
	lda	shnflg
	cmp	#$ff
	beq	shnmrt
	tax
	bmi	shnnak
;
	and	#1
	asl	a
	tax
	INDCAL	snmvtb,x
	jmp	shnman
;
snmvtb:
	JTBL	shnlef
	JTBL	shnrig
;
shnnak:
	lda	timer
	and	#1
	bne	shnnrt
	inc	shncnt
	beq	shnend
shnnrt:	rts	
;
shnlef:
	lda	shinxl
	sec
	sbc	#2
	sta	shinxl
	lda	shinxh
	sbc	#0
	sta	shinxh
;
	bcc	shnled	
;
	lda	shinxl
	cmp	scrolh
	lda	shinxh
	sbc	scrole
	bcs	shnlrt
;
shnled:
	lda	scrolh
	sta	shinxl
	lda	scrole
	sta	shinxh
	inc	shnflg
	lda	shnflg
	cmp	#6
	bcs	shnend
;
shnlrt:	rts
;
shnrig:
	lda	shinxl
	clc
	adc	#2
	sta	shinxl
	lda	shinxh
	adc	#0
	sta	shinxh
;
	lda	shinxl
	sec
	sbc	scrolh
	tax
	lda	shinxh
	sbc	scrole
	cpx	#$f1
	sbc	#0
	bcc	shnrrt
;
	lda	scrolh
	clc
	adc	#$f0
	sta	shinxl
	lda	scrole
	adc	#0
	sta	shinxh
	inc	shnflg
shnrrt:	rts
;
shnend:
	lda	#0
	sta	effcmd
	sta	naksfl
	ldx	#GIRARU
	lda	#0
	sta	ninsta,x
;
	lda	#(GIRARU + 1) or $80
	jsr	ninpsb
;	sta	ninodr
	lda	#$ff
	sta	shnflg
shnert:	rts
;
;
shnman:	;hattori kun tono atari
	lda	shnflg
	bmi	shnert	;snmret
;
	lda	shiny
	clc
	adc	#15
	sec
	sbc	many
	cmp	#24+15
	bcs	shnert	;snmret
;
	lda	shinxl
	clc
	adc	#15
	tax
	lda	shinxh
	adc	#0
	tay
	txa
	sec
	sbc	manxl
	tax
	tya
	sbc	manxh
	cpx	#16+15
	sbc	#0
	bcs	shnert
snmnok:	lda	#$0b
	sta	effcmd	;	jsr	effset
	lda	#1
	sta	naksfl
	lda	#$80
	sta	shnflg
	sta	shncnt
	lda	#8
	jsr	scradd
;
	ldx	#TEKMAX-1
;
grtklp:	lda	tekflg,x
	cmp	#$ff
	beq	grtksk
	cmp	#16
	beq	grtksk
;
	cmp	#$e
	bcc	grtkok
	bne	grtk10
;kemu
	lda	tekdir,x
	ora	#%0100_1000
	sta	tekdir,x
	jmp	grtksk
grtk10:	;haku
	lda	teksta,x
	cmp	#$2
	bne	grtksk
	tay
	bne	grtkok
	lda	#$ff
	sta	tekflg,x
	bne	grtksk	;<bra>
;
grtkok:	
	lda	tekflg,x
	cmp	#1
	beq	grtkjj	
	cmp	#15
	bne	grtkkk
grtkjj:
	lda	#0
	sta	teknof
;
grtkkk:	lda	#16
	sta	tekflg,x
	lda	#30
	sta	teksta,x
;
grtksk:	dex
	bpl	grtklp
;machine ninja
	lda	mchflg
	bne	grmcrt
	lda	#0
	sta	mchflp
	inc	mchflg
grmcrt:
snmret:	rts
;
;
shndsp:	
	ldy	shnflg	;	lda	shnflg
	iny		;	cmp	#$ff
	beq	snmret
;
;	lda	shnflg
;	and	#1
;	asl	a
;	asl	a
;	asl	a
;	sta	pptr+0
;
	lda	timer
	and	#4
;	clc
;	adc	pptr+0
	clc
	adc	#low shnpat
	sta	pptr+0
	lda	#0
	adc	#high shnpat
	sta	pptr+1
;
	lda	shnflg
	bpl	shnds0
	lda	#8	;16	
	clc
	adc	pptr+0
	sta	pptr+0
	lda	#0
	adc	pptr+1
	sta	pptr+1
;
shnds0:
	lda	shinxl
	sta	pxl
	lda	shinxh
	sta	pxh
	lda	shiny
	sta	py
	lda	#19	;24	;17
	sta	poam
	lda	shnflg
	ror	a
	ror	a
	ror	a
	and	#$40
	ora	#$01
	sta	pcol		
	jmp	put2x2
;
;
shnpat:
;left
	db	$0e,$0f,$1e,$1f
	db	$2e,$2f,$3e,$3f
;right
;	db	$0f,$0e,$1f,$1e
;	db	$2f,$2e,$3f,$3e
;naki left
	db	$2a,$2b,$3a,$3b
	db	$2c,$2d,$3c,$3d
;naki right
;	db	$2b,$2a,$3b,$3a
;	db	$2d,$2c,$3d,$3c

