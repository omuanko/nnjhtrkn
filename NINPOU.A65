kansrt:
kirmov:
	lda	timer
	and	#3
	bne	kirret
;
	lda	kirflg
	beq	kirret
	ldx	kircnt
	dex
	stx	kircnt
	bne	kirret
	ldx	#KIRIGA
	lda	#0
	sta	kirflg
	sta	ninsta,x
	lda	#(KIRIGA + 1) or $80
	jsr	ninpsb
;	sta	ninodr	
kirret:	
kummov:	lda	timer
	and	#3
	bne	kummrt
	lda	kumflg
	beq	kummrt
	ldx	kumcnt
	dex
	stx	kumcnt
	bne	kummrt
	ldx	#KUMONO
	lda	#0
	sta	kumflg
	sta	ninsta,x
	lda	#(KUMONO + 1) or $80
	jsr	ninpsb
;	sta	ninodr	
