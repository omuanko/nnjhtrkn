;================================================================
;
tamset:
	lda	joyone	;1a
	bit	tambtn	;joyabt
	bne	sttm03
	lda	#0
	sta	tmdcnt	;hanasare
sttm03:	ldx	tmdcnt
	beq	sttm01
	dex
	stx	tmdcnt
sttm01:	ldx	trptcn
	beq	sttm02
	dex
	stx	trptcn
sttm02:
	lda	mandir
	and	#$10
	bne	sett10
;
sttm00:
	lda	tmdcnt
	ora	trptcn
	bne	settrt	
	lda	joyone	;1a
	bit	tambtn	;joyabt
	beq	settrt
	lda	trptcn
	bne	settrt
	lda	mandir
	ora	#$10
	sta	mandir
	lda	#4	;****
	sta	tmscnt
	lda	tamnxt	;#30	;15	;30	;1 sec
	sta	tmdcnt
	lda	tamrpt	;#16	;8	;16
	sta	trptcn
;
	jsr	tamgen
sett10:
	ldx	tmscnt	
	dex
	stx	tmscnt	
	bpl	settrt
	lda	mandir
	and	#$ef
	sta	mandir
settrt:
	rts
tamgen:	;tama genelate
	ldx	tmgmax
;
tmglop:	ldy	tamafl,x	;	lda	tamafl,x
	iny			;	cmp	#$ff
	beq	tamgok
	dex
	bpl	tmglop
	rts
;
tamgok:
	lda	#3
	jsr	effset
	lda	mandir
	lsr	a
	and	#1
	sta	tamafl,x
;
tamgxy:	lda	manxl
	clc
	adc	#4
	sta	tamaxl,x
	lda	manxh
	adc	#0
	sta	tamaxh,x	
;
	lda	many
	clc
	adc	#8
	sta	tamay,x
;
	lda	#0
	sta	tamacn,x
	rts
;
;midare
midgen:
	lda	timer
	and	#3
	bne	midgn0
	ldx	midtim
	dex
	stx	midtim
	beq	midend
midgn0:
	lda	joyone
	bit	tambtn
	beq	mtmgrt
	ldx	#0
mtmglp:	ldy	tamafl,x	;	lda	tamafl,x
	iny			;	cmp	#$ff
	beq	mtmgok
	inx
	cpx	#TAMMAX
	bcc	mtmglp
mtmgrt:	rts
;
mtmgok:
	lda	#3
	jsr	effset
	ldy	mtmcnt
	lda	mtmdat,y
	sta	tamafl,x
	iny
	sty	mtmcnt
	cpy	#5
	bcc	mtmgos
	ldy	#0
	sty	mtmcnt
mtmgos:	jmp	tamgxy
;
midend:	
	ldx	abtfnc
	ldy	midedo,x
	beq	midert
	lda	#0
	sta	ninsta,y
	sta	abtfnc
	iny
	tya
	ora	#$80
	jsr	ninpsb
	lda	#30	;30
	sta	tamnxt
	lda	#8	;16
	sta	tamrpt
midert:	rts	
;
mtmdat:
	db	0,2,4,3,1
;
midedo:
	db	NOMAL,MIDARE,HYPER
;
hypers:
	lda	timer
	and	#3
	bne	nin3go
	ldx	midtim
	dex
	stx	midtim
	beq	midend
nin3go:	jmp	tamset
