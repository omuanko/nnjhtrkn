;
;shishimaru
;
dogini:
	lda	#$ff
	sta	dogflg
	rts
;
dogmov:
	lda	dogflg
	cmp	#$ff
	beq	dogmrt
	tax
	beq	dogdwn	;0
	dex
	beq	dogmch	;1
	dex
	bne	dogmrt
	jmp	doghbs	;	beq	doghbs	;2
dogmrt:	rts
;
dogdwn:	;dog shutsugen
	lda	dogy
	clc
	adc	#4
	sta	dogy
;
	cmp	#$b0
	bcc	dgdret
	ldx	dogflg
	stx	dogcnt	;dogcnt = 0
	inx
	stx	dogflg
dgdret:	jmp	dogman
;
dogmch:	;dog matsu
	jsr	dogtim
	beq	dogend
dogman:	lda	dogy
	clc
	adc	#15
	sec
	sbc	many
	cmp	#24+15
	bcs	dgmret
;
	lda	dogxl
	clc
	adc	#15
	tax
	lda	dogxh
	adc	#0
	tay
	txa
	sec
	sbc	manxl
	tax
	tya
	sbc	manxh
	cpx	#16+15
	sbc	#0
	bcs	dgmret
;
	lda	#0
	sta	dogcnt
	sta	dogdir	
	sta	dghsta
	ldx	#2
	stx	dogflg
	inx	;lda	#3
	stx	muscmd
dgmret:	rts
;
dogend:	lda	#$ff
	sta	dogflg
	lda	#2
	sta	muscmd
	ldx	#HIBASI
	lda	#0
	sta	ninsta,x
	lda	#(HIBASI + 1) or $80
	jmp	ninpsb
;
;
;dghsta(hibashiri no stat.)
;bit0:kao no muki,bit1:tikazuku muki,bit2:hibashiri lock
doghbs:	;dog hibasiri
	jsr	dogtim
	beq	dogend
;
	lda	many
	clc
	adc	#8
	sta	dogy
	lda	dghsta	;man to dog no muki o check
	eor	mandir	;man(1)=dog(1)?
	and	#2	;sono bit o toridashi
	beq	dogh10	;onaji nara skip
;
	lda	mandir
	and	#2
	sta	dghsta
	lsr	a
	ora	dghsta
 	sta	dghsta
;
dogh10:	lda	dghsta
	bit	bit2st
	bne	doghst	
;
	bit	bit1st
	bne	dogh1r
;left mov
	lda	dogxl
	sec	
	sbc	#8
	sta	dogxl
	lda	dogxh
	sbc	#0
	sta	dogxh
	bcs	dgh1lo
	lda	#0
	sta	dogxl
	sta	dogxh	
dgh1lo:	lda	manxl
	sec
	sbc	dogxl
	tax
	lda	manxh
	sbc	dogxh
	bne	dgmwrt
	cpx	#$20
	sbc	#0
	bcc	dgmwrt
	lda	dghsta
	ora	#%0000_0100	
	sta	dghsta
	jmp	dgmwrt
dogh1r:
	lda	dogxl
	clc
	adc	#8
	sta	dogxl
	tax
	lda	dogxh
	adc	#0
	sta	dogxh
	cpx	#$f0
	sbc	#ARELEN-1
	bcc	dgh1ro
	lda	#$f0
	sta	dogxl
	lda	#ARELEN-1
	sta	dogxh
dgh1ro:	
	lda	dogxl
	sec
	sbc	manxl
	tax
	lda	dogxh
	sbc	manxh
	bne	dgmwrt
	cpx	#$20
	sbc	#0
	bcc	dgmwrt
	lda	dghsta
	ora	#%0000_0100	
	sta	dghsta
dgmwrt:	rts		

doghst:	;hibashiri lock go no move
	lda	dghsta
	and	#2
	bne	dghstr
dghstl:	;left stop
	jsr	tmfrdg	;ret Xreg,brk Areg
	stx	aa
	lda	manxl
	sec
	sbc	aa	;#$20
	sta	dogxl
	lda	manxh
	sbc	#0
	sta	dogxh
	bcs	dghlrt
	lda	#0
	sta	dogxl
	sta	dogxh
dghlrt:	rts
;
dghstr:	;right stop
	jsr	tmfrdg	;ret Xreg,brk Areg
	stx	aa
	lda	manxl
	clc
	adc	aa	;#$20
	sta	dogxl
	tax
	lda	manxh
	adc	#0
	sta	dogxh
	cpx	#$f0
	sbc	#ARELEN-1
	bcc	doghrt
	lda	#$f0
	sta	dogxl
	lda	#ARELEN-1
	sta	dogxh
doghrt:	rts
;
