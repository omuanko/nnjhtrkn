	lda	#0
prenl1:	
	sta	takflg,x
	dex
	bpl	prenl1
;
	rts
;
;
ninpsb:
	pha
	bit	bit7st
	beq	ninpjm
	lda	nincnt
	beq	ninpjm
	sec
	sbc	#1
	sta	nincnt
ninpjm:	pla
	jmp	nnpsub
;
;
ninres:	;ninpou reset
	ldx	#NINMAX-1
;
ninrlp:
	lda	ninsta,x
	cmp	#1
	beq	ninrok
	dex
	bne	ninrlp
	rts
;
ninrok:
	lda	#0
	sta	ninsta,x
	inx
	txa
	ora	#$80
	jmp	ninpsb		
;
;
ninent:
	ldx	#1	;bittbl
ninelp:	
	lda	ninsta,x
;	bit	bit0st
	beq	nineok
;	inc	aa
	inx
	cpx	#NINMAX
	bcc	ninelp
	rts
;
nineok:
	lda	#1
	sta	ninsta,x
	inx	
	txa
	jmp	ninpsb
;
ninfls:	;ninfwk
	ldx	ninfwk
	beq	ninfrt
	lda	#$29
	dex
	bne	ninfsk	
	lda	#0
	sta	selnfl
	lda	naksfl
	beq	ninfs0
	lda	#$b
	sta	effcmd
ninfs0:	lda	#$0f
ninfsk:	stx	ninfwk
	sta	colwrk+15
ninsrt:
