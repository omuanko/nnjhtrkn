ninset:
;
	lda	joyone
	bit	tambtn
	beq	ninpo0
;
	ldx	curpos
	beq	ninp00	;o0
;
	lda	scrole
	cmp	#ARELEN-1
	beq	ninfrt
;
	lda	ninsta,x
	and	#$3
	eor	#%0000_0001
	bne	ninsrt
;
	ldx	usemax
	lda	nincnt
	cmp	usenin,x	;#3
	bcs	ninsrt
;
	inc	nincnt
	ldx	curpos
	stx	cc
	lda	#2
	sta	ninsta,x
	jsr	ninfst
;
;	ldy	#2
;	ldx	cc
;	cpx	#5
;	bcs	*+4
;	ldy	#3
;	tya
;	lda	#2
;	sta	ninsta,x
;	txa
	lda	cc
	asl	a
	tax
	INDJMP	nincal,x
;
usenin:
	db	2,3,4,NINMAX
;
nincal:	JTBL	ninsrt	;0 nomal
	JTBL	ninpo1	;1 hyper run
	JTBL	ninpo6	;2 high jump
	JTBL	ninpob	;3 mizukumo
	JTBL	ninpo3	;4 hyper shuriken
	JTBL	ninpo4	;5 kanashibari
	JTBL	ninpo8	;6 kirigakure
	JTBL	ninpoa	;7 kumonori
	JTBL	ninpo5	;8 shinzou naku
	JTBL	ninpo7	;9 musasabi
	JTBL	ninpo9	;10hibashiri
	JTBL	ninpo2	;11midareuchi
;
;
ninp00:	jsr	chkie
ninpo0:	;*nomal tama
	lda	abtfnc	;shuriken shurui
	cmp	#3
	bcc	nin0ok	;bcs	nintrt
	rts
nin0ok:	asl	a
	tax
	INDJMP	nn0vec,x		
;
nn0vec:
	JTBL	tamset
	JTBL	midgen
	JTBL	hypers
;
ninpo1:	;+hyper run
	lda	#1
	sta	hypodr
	rts
;
ninpo2:	;*midare uchi
	lda	#1	;midareuch
	sta	abtfnc
	lda	#0
	sta	mtmcnt	;midare pos set
	lda	#$c0
	sta	midtim	;midara jizoku time
	rts
;
ninpo3:	;*hyper shuriken
	lda	#2	;hyper shot
	sta	abtfnc
	lda	#15
	sta	tamnxt
	lda	#4	;8
	sta	tamrpt
	lda	#$c0
	sta	midtim	;hyper jizoku time
	rts
;
ninpo4:	;+kanashibari
	lda	#$ff
	sta	kanflg
	rts
;
ninpo5:	;-shinzou naki (girauru)
	lda	#0
	sta	shnflg
	lda	#$b0
	sta	shiny
	lda	scrolh
	clc
	adc	#$fe
	sta	shinxl
	lda	scrole
	adc	#0
	sta	shinxh
	rts
;
ninpo6:	;+high jump
	lda	#1
	sta	takodr
	rts
;
ninpo7:	;+musasabi
	ldx	#1
	stx	mssflg
	dex
	stx	msscnt
	rts
;
ninpo8:	;+kirigakure
	ldx	#1
	sta	kirflg
	dex
	stx	kircnt
	rts
;
ninpo9:	;-hibashiri shishimaru
	lda	#0
	sta	dogflg
	lda	manxl
	sta	dogxl
	lda	manxh
	sta	dogxh
	lda	#0
	sta	dogy
n9r:	rts
;
ninpoa:	;kumonori
	ldx	#1
	stx	kumflg
	dex	;ldx	#0
	stx	kumcnt
	stx	mnyrps
	rts
;
ninpob:	;mizu kumo
	lda	#1
	sta	mizflg
	rts
